{
  "name": "redboer/starfield",
  "description": "This plugin adds a star rating field type to Craft CMS.",
  "version": "3.0.0",
  "type": "craft-plugin",
  "keywords": [
    "html",
    "cms",
    "craftcms",
    "yii2",
    "star",
    "stars"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Richard de Boer",
      "homepage": "https://github.com/redboer"
    }
  ],
  "support": {
    "issues": "https://github.com/redboer/craft-starfield/issues?state=open",
    "source": "https://github.com/redboer/craft-starfield",
    "docs": "https://github.com/redboer/craft-starfield"
  },
  "require": {
    "craftcms/cms": "^5.0.0",
    "php": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "redboer\\starfield\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "redboer\\starfield\\tests\\": "tests/"
    }
  },
  "extra": {
    "name": "Starfield",
    "handle": "starfield",
    "documentationUrl": "https://github.com/redboer/craft-starfield"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "craftcms/rector": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "scripts": {
    "phpstan": "phpstan analyse src --memory-limit=1G"
  }
}
